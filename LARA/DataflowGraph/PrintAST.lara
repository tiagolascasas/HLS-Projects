aspectdef PrintAST
	input
		root
	end
	printLevel(root, 0);
end

function printLevel(node, n) {
	println(getSpaces(n) + node.joinPointType);
	for (var i = 0; i < node.children.length; i++)
	printLevel(node.children[i], n + 1);
}

function getSpaces(n) {
	var s = "";
	for (i = 0; i < n; i++)
		s += "  ";
	return s;
}